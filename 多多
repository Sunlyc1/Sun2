[Script]
http-response https://api.duoduo.pro/user/personal/information script-path=https://raw.githubusercontent.com/Sunlyc1/Sun2/main/多多, requires-body=1,max-size=0
 

[Mitm]
hostname  =  nz-api.duoduo.pro 

var body = $response.body; 
var obj = JSON.parse(body); 

obj.data.user.vipInfo.level = 7
obj.data.user.vipInfo.valid = true
obj.data.user.vipInfo.expiredTime =  1776307200

body = JSON.stringify(obj);
$done({body});
